<% if @episode.errors.any? %>
  <div id="error_explanation">
    <div class="alert alert-error">
      The upload contains <%= pluralize(@episode.errors.count, "error") %>.
    </div>
    <ul>
    <% @episode.errors.full_messages.each do |msg| %>
      <li>* <%= msg %></li>
    <% end %>
    </ul>
  </div>
<% end %>
